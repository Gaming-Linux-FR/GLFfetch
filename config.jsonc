    {
        "type": "command",
        "shell": "/bin/bash",
        "key": "Installation date",
        "text": "echo $(stat -c %W /) | awk '{print strftime(\"%e/%m/%Y\",$1)}'"
    },
    "break",
    "colors"
  ]
}
